= bootstrap_form_for release, url: admin_release_path(release.id) do |f|
  = f.text_field :title
  = f.text_field :artist
  = f.text_field :catalog_number
  = f.text_field :cover_src
  = f.text_field :cover_thumbnail_src
  = f.text_field :large_cover_src
  = f.text_field :release_date
  = f.text_field :juno_link
  = f.text_field :itunes_link
  = f.text_field :beatport_link
  = f.text_field :bandcamp_link
  = f.text_area :description

  = f.submit
